<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>NativeAppOfUrl</name>
    <description>
description...    
</description>
    <author email="zeev199@gmail.com" href="zeev199@gmail.com">
        zeev Levi Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://rest.jdn.co.il/*" />
    <allow-navigation href="https://rest.jdn.co.il/*" />
    <allow-navigation href="https://fonts.googleapis.com/*" />
    <allow-navigation href="https://maxcdn.bootstrapcdn.com/*" />
    <allow-navigation href="https://www.googletagmanager.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="waze:*" />
    <allow-intent href="whatsapp:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
